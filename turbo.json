{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": [".dist/**"]
        },
        "test": {
            "dependsOn": ["build"],
            "inputs": ["src/**/*.{ts,tsx}", "__tests__/**/*.{ts,tsx}"]
        }
    }
}
